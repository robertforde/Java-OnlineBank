<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>doIndex</servlet-name>
        <servlet-class>com.onlinebank.controller.doIndex</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doRegister</servlet-name>
        <servlet-class>com.onlinebank.controller.doRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doAccount</servlet-name>
        <servlet-class>com.onlinebank.controller.doAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doAddAccount</servlet-name>
        <servlet-class>com.onlinebank.controller.doAddAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doCheckLogin</servlet-name>
        <servlet-class>com.onlinebank.controller.doCheckLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doDeposit</servlet-name>
        <servlet-class>com.onlinebank.controller.doDeposit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doDepositTrans</servlet-name>
        <servlet-class>com.onlinebank.controller.doDepositTrans</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doWithdraw</servlet-name>
        <servlet-class>com.onlinebank.controller.doWithdraw</servlet-class>
    </servlet>
