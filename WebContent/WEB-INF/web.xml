        <servlet-class>com.onlinebank.controller.doGetBalance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doGetBalanceFinal</servlet-name>
        <servlet-class>com.onlinebank.controller.doGetBalanceFinal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doTransfer</servlet-name>
        <servlet-class>com.onlinebank.controller.doTransfer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doTransferTrans</servlet-name>
        <servlet-class>com.onlinebank.controller.doTransferTrans</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doViewReport</servlet-name>
        <servlet-class>com.onlinebank.controller.doViewReport</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doLogOff</servlet-name>
        <servlet-class>com.onlinebank.controller.doLogOff</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>doIndex</servlet-name>
        <url-pattern>/index.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doRegister</servlet-name>
        <url-pattern>/register.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doAccount</servlet-name>
        <url-pattern>/Account.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doAddAccount</servlet-name>
        <url-pattern>/AddAccount.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doCheckLogin</servlet-name>
        <url-pattern>/logincheck.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doDeposit</servlet-name>
        <url-pattern>/deposit.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doDepositTrans</servlet-name>
        <url-pattern>/DepositTrans.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doWithdraw</servlet-name>
        <url-pattern>/withdraw.do</url-pattern>
    </servlet-mapping>
