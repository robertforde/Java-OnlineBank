        <servlet-name>doTransfer</servlet-name>
        <url-pattern>/transfer.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doTransferTrans</servlet-name>
        <url-pattern>/TransferTrans.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doViewReport</servlet-name>
        <url-pattern>/view-report.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doLogOff</servlet-name>
        <url-pattern>/logoff.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.do</welcome-file>
    </welcome-file-list>
</web-app>
